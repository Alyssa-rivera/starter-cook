			var $delete = '';

			if(gdasac_li_type != ''){
				if($type=='category'){
					output += '<li class="gdas-section-title" ontouchstart="this.click()" onclick="var event = arguments[0] || window.event; geodir_cancelBubble(event);">Categories</li>';
